$("body").removeClass("on-side");
});
},
hoverDropdown:function(){
var getNav=$("nav.navbar.bootsnav"),
getWindow=$(window).width(),
getHeight=$(window).height(),
getIn=getNav.find("ul.nav").data("in"),
getOut=getNav.find("ul.nav").data("out");
